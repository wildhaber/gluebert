{
    "plugins": [
        "transform-async-to-generator",
        "dynamic-import-webpack"
    ],
    "env": {
        "general": {
            "presets": [
                "stage-2",
                [
                    "env",
                    {
                        "targets": {
                            "modules": false,
                            "useBuiltIns": true,
                            "browsers": [
                                "> 1%",
                                "last 2 versions",
                                "Firefox ESR",
                                "IE 11"
                            ]
                        }
                    }
                ]
            ]
        },
        "karma": {
            "presets": [
                "stage-2",
                [
                    "env",
                    {
                        "targets": {
                            "modules": false,
                            "useBuiltIns": true,
                            "browsers": [
                                "last 2 versions",
                                "IE 11"
                            ]
                        }
                    }
                ]
            ]
        }
    }
}